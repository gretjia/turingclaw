{
  "name": "react-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx server.ts",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "test:contracts": "tsx tests/new/manifold_contract.ts && tsx tests/new/runtime_contract.ts && tsx tests/new/control_plane_guard_contract.ts && tsx tests/new/halt_protocol_contract.ts && tsx tests/new/progress_watchdog_contract.ts",
    "test:agi-mini": "tsx tests/new/agi_benchmark_suite.ts",
    "test:agi-resilience": "tsx tests/new/agi_benchmark_resilience.ts",
    "test:control-real-llm": "tsx tests/new/control_benchmark_real_llm.ts",
    "bench:os-longrun": "tsx tests/new/control_benchmark_real_llm.ts",
    "test:control-baseline": "tsx tests/new/control_benchmark_real_llm.ts --repeats 3 --freeze-baseline",
    "test:control-gate": "tsx tests/new/control_benchmark_real_llm.ts --repeats 2 --gate",
    "test:bench-all": "npm run test:contracts && npm run test:agi-mini && npm run test:agi-resilience",
    "start": "node server.ts",
    "cli": "tsx cli.ts",
    "telegram": "tsx telegram.ts"
  },
  "dependencies": {
    "@google/genai": "^1.42.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.14",
    "@vitejs/plugin-react": "^5.0.4",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "openai": "^6.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "vite": "^6.2.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
